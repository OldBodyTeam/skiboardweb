{
  "name": "ski-board-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-api": "pnpm dlx swagger-typescript-api -p https://www.ski-api.gawtec.com/swagger-doc-json -o ./src/services -n Client.ts --modular --axios",
